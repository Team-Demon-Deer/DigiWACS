<Window x:Class="DigiWACS.Client.MainWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:wpf="clr-namespace:Mapsui.UI.Wpf;assembly=Mapsui.UI.Wpf"
		mc:Ignorable="d"
		Title="DigiWACS" Height="450" Width="800">
    <DockPanel>
        <Menu DockPanel.Dock="Top" VerticalContentAlignment="Stretch">
            <MenuItem Header="_File">
                <MenuItem Header="_New Connection" />
                <MenuItem Header="_Open Connection" />
                <MenuItem Header="_Save Connection" />
                <Separator />
                <MenuItem Header="_Exit" />
            </MenuItem>
            <MenuItem Header="Test Commands">
                <MenuItem Header="NewWindow" Click="NewWindowButton_OnClick" />
                <MenuItem Header="gRPC Connect" Click="gRPCConnect_OnClick" />
            </MenuItem>
        </Menu>
        <wpf:MapControl x:Name="MapControl1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
    </DockPanel>
</Window>